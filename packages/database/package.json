{
  "name": "@mag-system/database",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "prisma/schema.prisma"
  ],
  "scripts": {
    "prisma:validate": "dotenv -e ../../.env -- prisma validate",
    "build": "dotenv -e ../../.env -- prisma generate && tsc",
    "prisma:format": "dotenv -e ../../.env -- prisma format",
    "prisma:migrate:prod": "dotenv -e ../../.env.production -- prisma migrate deploy",
    "prisma:seed:prod": "dotenv -e ../../.env.production -- tsx prisma/seed/index.ts",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "type-check": "tsc --noEmit",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:seed": "dotenv -e ../../.env -- tsx prisma/seed/index.ts",
    "prisma:reset": "dotenv -e ../../.env -- prisma migrate reset",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "prisma:migrate:resolve": "dotenv -e ../../.env.production -- prisma migrate resolve",
    "prisma:migrate:status": "dotenv -e ../../.env.production -- prisma migrate status"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@mag-system/eslint-config": "workspace:*",
    "prisma": "^6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6"
  }
}
