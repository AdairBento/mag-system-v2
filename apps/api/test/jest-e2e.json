{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testMatch": ["**/test/e2e/*.e2e-spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {}]
  },
  "moduleNameMapper": {
    "^@mag-system/(.*)$": "<rootDir>/../../packages/$1/src",
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}
